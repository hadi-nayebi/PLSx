{
        "name": "arch70",
        "type": "AECSS",
        "components": {
                "vectorizer": [
                        {"type": "Linear", "in_features": 21, "out_features": 8, "init": "xavier", "activation": "Tanh"}
                ],
                "encoder":[
                        {"type": "Conv1d", "in_features": 8, "out_features": 64, "kernel": 5, "padding": "same", "init": "xavier", "activation": "Tanh"},
                        {"type": "Conv1d", "in_features": 64, "out_features": 64, "kernel": 5, "padding": "same", "init": "xavier", "activation": "Tanh"},
                        {"type": "MaxPool1d", "kernel": 2},
                        {"type": "Conv1d", "in_features": 32, "out_features": 64, "kernel": 5, "init": "xavier", "activation": "Tanh"},
                        {"type": "Flatten"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "xavier", "activation": "Tanh"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "xavier", "activation": "Tanh"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "xavier", "activation": "Tanh"},
                        {"type": "Linear", "in_features": 1024, "out_features": 10, "init": "xavier", "activation": "Tanh"}
                ],
                "decoder": [
                        {"type": "Linear", "in_features": 10, "out_features": 1024, "init": "he", "activation": "ReLU"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "he", "activation": "ReLU"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "he", "activation": "ReLU"},
                        {"type": "Linear", "in_features": 1024, "out_features": 1024, "init": "he", "activation": "ReLU"},
                        {"type": "Unflatten", "in_features": 64, "out_features": 16},
                        {"type": "ConvTranspose1d", "in_features": 64, "out_features": 64, "kernel": 5, "init": "he", "activation": "ReLU"},
                        {"type": "ConvTranspose1d", "in_features": 64, "out_features": 64, "kernel": 5, "padding": 2, "init": "he", "activation": "ReLU"},
                        {"type": "ConvTranspose1d", "in_features": 64, "out_features": 8, "kernel": 5, "padding": 2, "init": "he", "activation": "ReLU"}
                ],
                "ss_decoder": [
                        {"type": "Linear", "in_features": 10, "out_features": 256, "init": "he", "activation": "ReLU"},
                        {"type": "Linear", "in_features": 256, "out_features": 256, "init": "he", "activation": "Sigmoid"},
                        {"type": "Unflatten", "in_features": 32, "out_features": 8},
                        {"type": "ConvTranspose1d", "in_features": 32, "out_features": 64, "kernel": 6, "init": "he", "activation": "ReLU"},
                        {"type": "ConvTranspose1d", "in_features": 64, "out_features": 9, "kernel": 8, "init": "xavier", "activation": "LogSoftmax"}
                ],
                "classifier": [
                        {"type": "Linear", "in_features": 10, "out_features": 1024, "init": "he", "activation": "ELU"},
                        {"type": "Linear", "in_features": 1024, "out_features": 2, "init": "xavier", "activation": "Softmax"}
                ],
                "devectorizer": [
                        {"type": "Linear", "in_features": 8, "out_features": 21, "init": "xavier", "activation": "LogSoftmax"}
                ]
        },
        "connections":[
                ["vectorizer", "encoder"],
                ["encoder", "decoder"],
                ["encoder", "ss_decoder"],
                ["encoder", "classifier"],
                ["decoder", "devectorizer"]
        ]
}